//art
if (art&gt;-1 &amp;&amp;alph_art&gt;0) {
    var bh=background_get_height(art);
    var bw=background_get_width(art);
    var scale=AP_winw/bw;
    bh*=scale;
    bw*=scale;
    draw_set_alpha(alph_art)
    
    draw_background_stretched(art,0,AP_winh/2-bh/2,bw,bh)
}

draw_set_alpha(1);

//letterboxing
if (letter &gt; 0) {
    draw_set_color(c_black);
    draw_rectangle(0,0,AP_winw,letter*(AP_centre-AP_barh),false);
    draw_rectangle(0,AP_winh*(1-letter)+letter*(AP_centre+AP_barh),AP_winw,AP_winh,false);
}

//text
if (alph_words&gt;0) {
    draw_set_halign(fa_center)
    draw_set_color(c_white);
    draw_set_alpha(alph_words);
    chnm = campaign.chapter_names[campaign.chapter];
    draw_text(AP_textx,AP_texty,chnm)
}

draw_set_alpha(1)
</string>

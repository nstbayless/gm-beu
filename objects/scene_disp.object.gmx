            <string>var alph_words=0;
var alph_art=0;
var prog_art=0;
var letter=0
var delay_in = AN_interlude_delay*interlude;

//letterboxing
if (anim&lt;AN_letterdone) {
    if (anim&gt;AN_letterstart)
        letter=(anim-AN_letterstart)/(AN_letterdone-AN_letterstart)
} else if (anim&lt;AN_letterout+delay_in) {
    letter=1
} else if (anim&lt;AN_lettergone+delay_in) {
    letter = 1-(anim-AN_letterout-delay_in)/(AN_lettergone-AN_letterout)
}

//art alpha
if (anim&lt;0&amp;&amp;anim&gt;AN_artstart)
    alph_art=1-anim/AN_artstart;
else if (anim&lt;AN_artout+delay_in)
    alph_art=1;
else if (anim&lt;AN_artgone+delay_in)
    alph_art=1-(anim-AN_artout-delay_in)/(AN_artgone-AN_artout)

//art x
if (anim&gt;0)
    prog_art=anim/(AN_artgone+delay_in)
    
if (letter&gt;0)
    letter=sin(pi*letter/2)
else
    letter=sin(pi*letter);

//words alpha
if (anim &gt; 0 &amp;&amp; anim &lt; AN_wordsin) {
    alph_words=anim/AN_wordsin
} else if (anim&gt;=AN_wordsin &amp;&amp; anim &lt;= AN_wordsout+delay_in) {
    alph_words=1;
} else if (anim&gt;=AN_wordsout+delay_in &amp;&amp; anim &lt;= AN_wordsgone+delay_in) {
    alph_words = 1-(anim-AN_wordsout-delay_in)/(AN_wordsgone-AN_wordsout)
}
</string>

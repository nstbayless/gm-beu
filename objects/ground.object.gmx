        strip.z=z
        strip.surf=strip_surf;
        strip.surf_w=surf_w;
        strip.surf_h=strip_thickness
        strip.parent=id;
        surface_reset_target();
        strips[i]=strip
        strips[i+1]=noone;
    }
    surface_reset_target();   
    surface_free(surf);
}

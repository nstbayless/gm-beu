            <string>//last element encountered on first traversal:
var last_element=-1;
if (instance_exists(room_properties))
    for (var i=0;i&lt;spawn_n;i++) {
        spawn_timer[i]--;
        if spawn_player[i]&gt;=0 {
            last_element=i;
            if (spawn_timer[i]&lt;=0) {
                var player = spawn_player[i];
                var pc_inst = instance_create_xyz(ch_spawn.x,ch_spawn.y,ch_spawn.z,pc_actor_obj[player]);
                pc_inst.player=player;
                obj_focus.subfocus[player]=pc_inst;
                pc_actor[player]=pc_inst;
                spawn_player[i]=-1;
            }
        }
    }

spawn_n=last_element+1;

//adjust number of players if gamepad inserted:

frame++;
if (frame%30==1) {
    var tmp_gamepad_count = get_gamepad_count()
    gamepad_count = tmp_gamepad_count;
    var tmp_pc_count = max(1,gamepad_count);
    if (tmp_pc_count!=pc_count) { 
        if (tmp_pc_count&gt;pc_count) {
            //add players to make up difference
            for (var i=pc_count;i&lt;tmp_pc_count;i++) {
                pc_actor[i]=noone;
                pc_actor_obj[i]=pc_main;
                //add to spawn list:
                spawn_timer[spawn_n]=0;
                spawn_player[spawn_n]=i;
                spawn_n++;
                if (instance_exists(obj_focus))
                    obj_focus.subfocus[i+1]=noone;
            }
        }
        if (tmp_pc_count&lt;pc_count) {
            //remove players
            for (var i=pc_count-1;i&gt;=tmp_pc_count;i--) {
                instance_activate_object(pc_actor[i])
                with (pc_actor[i]) {
                    instance_destroy();
                }
                pc_actor[i]=noone;
                pc_actor_obj[i]=noone
            }
        }
        pc_count=tmp_pc_count;
    }    
}
